  <bltx:entry id="uhlik-zele2018-voprosy" entrytype="article">
    <bltx:names type="author">
      <bltx:name>
        <bltx:namepart type="family" initial="U">Uhlik</bltx:namepart>
        <bltx:namepart type="given" initial="M">Mladen</bltx:namepart>
