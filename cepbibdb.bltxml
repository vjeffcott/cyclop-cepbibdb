  <bltx:entry id="uegaki2019-llcompass" entrytype="article">
    <bltx:names type="author">
      <bltx:name>
        <bltx:namepart type="family" initial="U">Uegaki</bltx:namepart>
        <bltx:namepart type="given" initial="W">Wataru</bltx:namepart>
