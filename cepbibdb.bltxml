  <bltx:entry id="djaerv2023-jsem" entrytype="article">
    <bltx:ids>
      <bltx:list>
        <bltx:item>djarv2023-jsem</bltx:item>
      </bltx:list>
    </bltx:ids>
    <bltx:names type="author">
      <bltx:name>
        <bltx:namepart type="family" initial="D">Djärv</bltx:namepart>
        <bltx:namepart type="given" initial="K">Kajsa</bltx:namepart>
      </bltx:name>
    </bltx:names>
