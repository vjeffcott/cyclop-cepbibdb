  <bltx:entry id="theiler-roelofsen-aloni2019-nls" entrytype="article">
    <bltx:names type="author">
      <bltx:name>
        <bltx:namepart type="family" initial="T">Theiler</bltx:namepart>
        <bltx:namepart type="given" initial="N">Nadine</bltx:namepart>
