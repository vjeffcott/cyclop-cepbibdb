  <bltx:entry id="landau2010-li" entrytype="article">
    <bltx:names type="author">
      <bltx:name>
        <bltx:namepart type="family" initial="L">Landau</bltx:namepart>
        <bltx:namepart type="given" initial="I">Idan</bltx:namepart>
      </bltx:name>
    </bltx:names>
